{
    "name": "windwalker/uri",
    "type": "windwalker-package",
    "description": "Windwalker Uri package",
    "keywords": [
        "windwalker",
        "framework",
        "http",
        "psr7",
        "psr17",
        "uri"
    ],
    "homepage": "https://github.com/ventoviro/windwalker-uri",
    "license": "MIT",
    "require": {
        "php": ">=8.0.0",
        "psr/http-message": "^1.0",
        "psr/http-factory": "^1.0",
        "windwalker/utilities": "4.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0||^9.0",
        "windwalker/test": "4.x-dev"
    },
    "suggest": {
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Windwalker\\Uri\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Uri\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-4": "4.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        }
    },
    "provide": {
        "psr/http-message-implementation": "~1.0",
        "psr/http-factory-implementation": "~1.0"
    }
}
